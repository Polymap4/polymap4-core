               label="%actions_deleteFeatures"
               style="toggle"
               toolbarPath="mouseMode"
               tooltip="%actions_deleteFeatures">
         </action-->
         <!--action
               class="org.polymap.core.mapeditor.edit.DrawFeatureEditorAction"
               icon="icons/etool16/new_line_mode.gif"
               id="org.polymap.core.mapeditor.action.drawFeatures_editor"
               label="%actions_drawFeatures"
               style="toggle"
               toolbarPath="mouseMode"
               tooltip="%actions_drawFeatures">
         </action-->
         <!--action
               class="org.polymap.core.mapeditor.edit.ModifyFeatureEditorAction"
               icon="icons/etool16/edit_vertex_mode.gif"
               id="org.polymap.core.mapeditor.action.modifyFeatures_editor"
               label="%actions_modifyFeatures"
               style="toggle"
               toolbarPath="mouseMode"
               tooltip="%actions_modifyFeatures">
         </action-->
         <!--action
               class="org.polymap.core.mapeditor.actions.CancelEditLayersAction"
               icon="icons/elcl16/remove_co.gif"
               id="org.polymap.core.mapeditor.action.cancelEdit_editor"
               label="%actions_cancelEdit"
               style="push"
               toolbarPath="mouseMode"
               tooltip="%actions_cancelEdit">
         </action-->
         <!--action
               class="org.polymap.core.mapeditor.edit.ModifyFeatureAction"
               icon="icons/etool16/edit_vertex_mode.gif"
               id="org.polymap.core.mapeditor.action.editFeatures_editor"
               label="%actions_editFeatures"
               style="toggle"
               toolbarPath="mouseMode"
               tooltip="%actions_editFeatures">
         </action-->
         <!--action
               class="org.polymap.core.mapeditor.selection.SelectFeatureEditorAction"
               icon="icons/etool16/layer_selectable.gif"
               id="org.polymap.core.mapeditor.action.selectFeatures_editor"
               label="%actions_selectFeatures"
               style="toggle"
               toolbarPath="mouseMode"
               tooltip="%actions_selectFeatures">
         </action-->
         <!--action
               class="org.polymap.core.mapeditor.navigation.PanZoomEditorAction"
               icon="icons/etool16/pan_mode.gif"
               id="org.polymap.core.mapeditor.action.panZoom_editor"
               label="%actions_panZoom"
               style="toggle"
               toolbarPath="mouseMode">
         </action-->
         <action
               class="org.polymap.core.mapeditor.tooling.navi.ForwardEditorAction"
               icon="icons/etool16/forward_nav.gif"
               id="org.polymap.core.mapeditor.action.mapForward_editor"
               label="%actions_mapForwardTip"
               style="push"
               toolbarPath="mouseMode"
               tooltip="%actions_mapForwardTip">
         </action>
         <action
               class="org.polymap.core.mapeditor.tooling.navi.BackwardEditorAction"
               icon="icons/etool16/backward_nav.gif"
               id="org.polymap.core.mapeditor.action.mapBack_editor"
               label="%actions_mapBackwardTip"
               style="push"
               toolbarPath="mouseMode"
               tooltip="%actions_mapBackwardTip">
         </action>
         <!--action
               class="org.polymap.core.mapeditor.actions.SetLayerBoundsAction"
               icon="icons/elcl16/zoom_layer_co.gif"
               id="org.polymap.core.mapeditor.ZoomToLayerAction"
               label="%actions_zoomToLayer"
               style="push"
               toolbarPath="mouseMode"
               tooltip="%actions_zoomToLayer">
         </action-->
         <action
               class="org.polymap.core.mapeditor.tooling.navi.MaxExtendEditorAction"
               icon="icons/etool16/zoom_extent_co.gif"
               id="org.polymap.core.mapeditor.action.mapMaxExtent_editor"
               label="%actions_mapMaxExtent"
               style="push"
               toolbarPath="mouseMode"
               tooltip="%actions_mapMaxExtentTip">
         </action>
      </editorContribution>
   </extension>
   <extension
         point="org.polymap.core.operation.concerns">
      <concern
            class="org.polymap.core.mapeditor.operations.OpenMapConcernFactory"
            id="org.polymap.core.mapeditor.OpenMapConcern">
      </concern>
   </extension>
   <extension
         point="org.polymap.core.mapeditor.mapContextMenu">
      <contribution
            class="org.polymap.core.mapeditor.contextmenu.LayerVisibleContribution"
            id="org.polymap.core.mapeditor.contextmenu.LayerVisibleContribution">
      </contribution>
      <contribution
            class="org.polymap.core.mapeditor.contextmenu.WmsFeatureInfoContribution"
            id="org.polymap.core.mapeditor.contextmenu.WmsFeatureInfo">
      </contribution>
   </extension>
   <extension
         point="org.eclipse.ui.elementFactories">
      <factory
            class="org.polymap.core.mapeditor.MapEditorInput"
            id="org.polymap.core.mapeditor.MapEditorInputFactory">
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="org.polymap.core.mapeditor.tooling.ToolingView"
            icon="icons/eview16/tooling2.gif"
            id="org.polymap.core.mapeditor.ToolingView"
            name="%views_tooling"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.polymap.core.mapeditor.editorTools">
      <tool
            class="org.polymap.core.mapeditor.tooling.navi.NavigationTool"
            description="%tools_navi_description"
            icon="icons/etool16/pan_mode.gif"
            id="org.polymap.core.mapeditor.NavigationTool"
            label="%tools_navi_label"
            toolPath="navigation"
            tooltip="%tools_navi_tip">
      </tool>
      <tool
            class="org.polymap.core.mapeditor.tooling.select.SelectionTool"
            description="%tools_select_description"
            icon="icons/etool16/layer_selectable.gif"
            id="org.polymap.core.mapeditor.SelectionTool"
            label="%tools_select_label"
            toolPath="selection"
            tooltip="%tools_select_tip">
      </tool>
      <tool
            class="org.polymap.core.mapeditor.tooling.edit.EditTool"
            description="%tools_edit_description"
            icon="icons/etool16/layer_editable.gif"
            id="org.polymap.core.mapeditor.EditTool"
            label="%tools_edit_label"
            toolPath="edit"
            tooltip="%tools_edit_tip">
      </tool>
      <tool
            class="org.polymap.core.mapeditor.tooling.edit.SnapTool"
            description="%tools_snap_description"
            icon="icons/etool16/snap_target.gif"
            id="org.polymap.core.mapeditor.EditSnapTool"
            label="%tools_snap_label"
            toolPath="edit/snap"
            tooltip="%tools_snap_tip">
      </tool>
      <tool
            class="org.polymap.core.mapeditor.tooling.edit.DigitizeTool"
            description="%tools_digitize_description"
            icon="icons/etool16/new_line_mode.gif"
            id="org.polymap.core.mapeditor.DigitizeTool"
            label="%tools_digitize_label"
            toolPath="digitize"
            tooltip="%tools_digitize_tip">
      </tool>
      <tool
            class="org.polymap.core.mapeditor.tooling.edit.SnapTool"
            description="%tools_snap_description"
            icon="icons/etool16/snap_target.gif"
            id="org.polymap.core.mapeditor.DigitizeSnapTool"
            label="%tools_snap_label"
            toolPath="digitize/snap"
            tooltip="%tools_snap_tip">
      </tool>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            adaptable="true"
            class="org.polymap.core.mapeditor.tooling.select.LayerSelectableDecorator"
            id="org.polymap.core.mapeditor.LayerSelectableDecorator"
            label="Layer Selectable"
            lightweight="true"
            objectClass="org.polymap.core.project.ILayer"
            state="true">
      </decorator>
      <decorator
            adaptable="true"
            class="org.polymap.core.mapeditor.tooling.edit.LayerEditableDecorator"
            id="org.polymap.core.mapeditor.LayerEditableDecorator"
            label="Layer Editable"
            lightweight="true"
            objectClass="org.polymap.core.project.ILayer"
            state="true">
      </decorator>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.polymap.core.project.dataPerspective">
         <viewShortcut
               id="org.polymap.core.mapeditor.ToolingView">
         </viewShortcut>
         <view
               closeable="true"
               id="org.polymap.core.mapeditor.ToolingView"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="net.refractions.udig.catalog.ui.CatalogView"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
