<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.polymap.core.operation.concerns" name="Operation Concerns" schema="schema/operation.concerns.exsd"/>
   <extension-point id="org.polymap.core.security.AuthorizationModule" name="AuthorizationModule" schema="schema/AuthorizationModule.exsd"/>
   <extension
         point="org.eclipse.ui.commands">
      <category
            description="POLYMAP Workbench Kategorie"
            id="org.polymap.core.workbench.category"
            name="Workbench Kategorie">
      </category>
   </extension>
